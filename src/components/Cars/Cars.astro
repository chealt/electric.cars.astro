---
import Car from './Car.astro';
import List from '../List/List.astro';
import ListItem from '../List/ListItem.astro';

import './Cars.css';

const { cars } = Astro.props;
---
<List class="cars">
  {cars.map(car => (
    <ListItem class="car" data-markets={car.markets.join(',')}>
      <Car {...car} />
    </ListItem>
  ))}
</List>
